

<security-constraint>
	<web-resource-collection>
		<web-resource-name>MyRealm</web-resource-name>
		<url-pattern>/*</url-pattern>
	</web-resource-collection>
	<auth-constraint>
		<role-name>manager</role-name>
	</auth-constraint>
</security-constraint>
<login-config>
	<auth-method>BASIC</auth-method>
	<realm-name>MyRealm</realm-name>
</login-config>
<security-role>
	<role-name>manager</role-name>
</security-role>